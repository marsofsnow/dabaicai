{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/opt/eosio.cdt/1.7.0/include/eosiolib/contracts",
                "/usr/opt/eosio.cdt/1.7.0/include/eosiolib/core",
                "/usr/opt/eosio.cdt/1.7.0/include",
                "${workspaceFolder}/"
            ],
            "defines": [
                "USING_ACTION",
                "USING_ACTION(contract_name,action_name)",
                "n"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "intelliSenseMode": "clang-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${default}",
                "path": [
                    "${default}",
                    "/usr/opt/eosio.cdt/1.7.0/include/"
                ]
            }
            //"configurationProvider": "go2sh.cmake-integration"
        }
    ],
    "version": 4
}